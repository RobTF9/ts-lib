{
  "compilerOptions": {
    "composite": true,
    "target": "ES2018",
    "module": "commonjs",
    "outDir": "dist",
    "declaration": true,
    "rootDir": "src",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "stripInternal": true,
    "types": [],
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
